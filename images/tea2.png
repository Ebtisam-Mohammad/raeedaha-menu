     function productOptionsChanged(selected_product) {
        if (selected_product) {

            $('#product-id').val(selected_product.id)

            if (!selected_product.unavailable) {
                // Selected a valid variant that is available.
                if(selected_product.formatted_sale_price){
                    $('#product-price').html(selected_product.formatted_sale_price);
                    $('#product-old-price').html(selected_product.formatted_price);
                    $('.add-to-cart-container').removeClass('d-none').addClass('d-flex');
                    $('.section-out-of-stock-notify-me').addClass('d-none');
                    $('#product-old-price').show();
                    $('#product-price').show();
                    $('#product-price-original').hide();
                }else{
                    $('#product-price-original').html(selected_product.formatted_price);
                    $('.add-to-cart-container').removeClass('d-none').addClass('d-flex');
                    $('.section-out-of-stock-notify-me').addClass('d-none');
                    $('#product-old-price').hide();
                    $('#product-price').hide();
                    $('#product-price-original').show();
                }

                if(selected_product.weight.value){
                    $('.div-product-weight').removeClass('d-none');
                    $('.div-product-weight .product-weight').html(selected_product.weight.value+' '+selected_product.weight.unit)
                }else{
                    $('.div-product-weight').addClass('d-none')
                }

                if(selected_product.sku){
                    $('.div-product-sku').removeClass('d-none');
                    $('.div-product-sku .product-sku').html(selected_product.sku)
                }else{
                    $('.div-product-sku').addClass('d-none');
                }

                $('.btn-add-to')
                    .removeClass('disabled')
                    .removeAttr('disabled')
                    .val('Add to Cart')
                    .fadeTo(200, 1);
            } else {
                // Variant is sold out.
                $('.btn-add-to')
                    .val('Sold Out')
                    .addClass('disabled')
                    .attr('disabled', 'disabled')
                    .fadeTo(200, 0.5);
                    // Variant is sold out.
                $('.select-quantity-div').addClass('d-none')
                $('.product-buttons').addClass('d-none');
                $('.section-out-of-stock-notify-me').removeClass('d-none');
                $('.add-to-cart-container').removeClass('d-flex').addClass('d-none');

                $('.btn-add-to-cart span').html('');
            }

        } else {
            // variant doesn't exist.
            $('#add')
                .val('Unavailable')
                .addClass('disabled')
                .attr('disabled', 'disabled')
                .fadeTo(200, 0.5);
            // variant doesn't exist.
                $('.select-quantity-div').addClass('d-none')
                $('.product-buttons').addClass('d-none');
                $('.section-out-of-stock-notify-me').removeClass('d-none');
                $('.add-to-cart-container').removeClass('d-flex').addClass('d-none');

                $('.btn-add-to-cart span').html('');
        }
    }

    function sendProductNotifyMe() {
        if(!$('.send-notify-progress').hasClass('d-none'))
            return;

        $('.send-notify-progress').removeClass('d-none');
        zid.store.product.setAvailabilityNotificationEmail($('.send-notify-product-id').val(),$('.send-notify-email').val()).then(function (response) {
            heading = response.status == 'success' ? 'success' : 'error';
            $.toast({
                heading: heading,
                text: 'تم الارسال بنجاح',
                icon: heading,
                position: 'top-right',
            })
            $('.send-notify-progress').addClass('d-none');
        })
    }
    document.addEventListener("DOMContentLoaded", function(){
        lightGallery(document.getElementById('product-details-slider'), {
            selector: '.lightgallery',
            plugins: [lgZoom, lgThumbnail],
            exThumbImage: 'data-external-thumb-image',
            mobileSettings:{ controls: false, showCloseIcon: true }
            //selectWithin: '.slick-slide:not(.slick-cloned)'
        });

        var productId = $('#product-id').val();
        var productIdreview = $('#product-id-review').val()

        function getProductReviews(id, page = 1) {
            zid.store.product.getReviews(id, page).then(function (response) {
                var
                    rating = response.data.details.rating,
                    reviews = response.data.reviews,
                    average = rating.average.toString();
                totalCount = rating.total_count,
                    ratings = (rating.average * 2) * 10;

                $('.based-on-count').text(totalCount)
                listComment(reviews, page)
                $('.main-ratings').width(ratings + '%')
                $('.main-average').text(average)
            })
        }


        function listComment(reviews, page) {
            var
                nextPageUrl = reviews.next_page_url,
                prevPageUrl = reviews.prev_page_url,
                allReviews = reviews.data,
                template = '',
                path = reviews.path + '?page=';

            $('.first-reviews').empty();
            $('.pagination-custom').empty();

            $.each(allReviews, function (index, review) {
                var
                    boughtThisItem = review.bought_this_item,
                    rating = (review.rating * 2) * 10
                avatar = $('.avatar').val();

                template +=
                    `<li>
                            <div class="comment">
                                <figure class="comment-media">
                                    <a href="javascript:void(0)">
                                        <img src="${avatar}" alt="avatar">
                                    </a>
                                </figure>
                                <div class="comment-body">
                                    <div class="mb-0 comment-rating ratings-container">
                                        <div class="ratings-full">
                                            <span class="ratings" style="width:${rating}%"></span>
                                            <span class="tooltiptext tooltip-top">${review.rating}</span>
                                        </div>
                                    </div>
                                    <div class="comment-user">
                                        <span class="comment-date text-body">${review.created_at}</span>
                                        <h4><a href="javascript:void(0)">${review.customer.name}</a></h4>
                                    </div>
                                    <div class="comment-content">
                                        <p>${review.comment}</p>
                                    </div>
                                </div>
                            </div>
                        </li>`
            });

            $('.first-reviews').html(template);

            var next = nextPageUrl ? nextPageUrl.replace(path, "") : '';
            var prev = prevPageUrl ? prevPageUrl.replace(path, "") : '';

            var templatePages =
                `<button  class="pagination-btn page-btn btn ${prev == '' ? 'btn-dark' : 'btn-primary'} btn-sm ml-4" page="${prev}"
                        ${prev == '' ? 'disabled' : ''}>
                        <i class="px-1 fa fa-angle-right"></i> السابق
                    </button>
                    <button class="pagination-btn mx-3 page-btn btn ${next == '' ? 'btn-dark' : 'btn-primary'}  btn-sm" page="${next}"
                        ${next == '' ? 'disabled' : ''}> التالى
                        <i class="px-1 fa fa-angle-left"></i>
                    </button>`

            $('.pagination-custom').html(templatePages);

        }

        function getProductReviewsOnly(id, page = 1) {
            zid.store.product.getReviews(id, page).then(function (response) {
                var reviews = response.data.reviews;
                listComment(reviews, page)
            })
        }

        $(document).on('click', '.pagination-btn', function () {
            var
                page = $(this).attr('page'),
                productId = $('#product-id-review').val();

            getProductReviewsOnly(id, page);
        })



        function addProductReview(id) {
            var
                comment = $('.product-comment').val(),
                auth = $('#auth').val(),
                rating = $('.product-stars-val').val(),
                notAuth = auth ? false : true,
                id = productId

            zid.store.product.addReview(id, comment, rating, notAuth)
                .then(function (response) {
                    heading = response.status == 'success' ? 'success' : 'error';
                    $.toast({
                        heading: heading,
                        text: response.data.message,
                        icon: heading,
                        position: 'top-right',
                    })
                })
        }



        fetchProduct(productId)
        getProductReviews(productIdreview)

        $(document).on('click', '.add-product-review', function () {
            var productId = $('#product-id-review').val();
            addProductReview(productId);
        })

        $(document).on('click', '.selected-star', function () {
            var rate = $(this).attr('rate');
            $('.product-stars-val').val(rate)
            $('.selected-star').removeClass('active');
            $(this).addClass('active')
           
        })


    });
</script>
